    "scripts": {
        "start-react": "cross-env BROWSER=none PORT=4000 react-scripts start",
        "start": "webpack serve --mode development --config webpack.react.standalone.js",
        "react-build": "webpack --mode production",
        "eject-react": "react-scripts eject",
        "build-react": "react-scripts build",
        "electron": "electron .",
        "electron-dev": "nodemon --signal SIGUSR2 --exec electron . --trace-warnings",
        "start-conc": "concurrently \"cross-env BROWSER=none \" \"npm run start-react\" \"npm run electron-dev\"",
        "start-conc:webpack": "concurrently \"cross-env BROWSER=none \" \"npm run start\" \"npm run electron-dev\"",
        "pack": "electron-builder --dir",
        "preelectron-pack": "yarn build",
        "electron-pack": "build --em.main=build/electron.js",
        "build": "react-scripts build && electron-builder",
        "dist": "electron-builder -w -c.extraMetadata.main=build/electron.js",
        "electron:package:win": "npx webpack --config webpack.config.js && electron-builder -w -c.extraMetadata.main=public/electron.js",
        "electron:package:mac": "npx webpack --config webpack.config.js && electron-builder -m -c.extraMetadata.main=public/electron.js",
        "electron:package:linux": "npx webpack --config webpack.config.js && electron-builder -l -c.extraMetadata.main=public/electron.js",
        "release": "electron-builder -p always",
        "docs": "jsdoc -c jsdoc.json",
        "build:custom-react": "webpack --config webpack.react.config.js",
        "build:custom-node": "webpack --config webpack.node.config.js"
    },
    "build": {
        "appId": "com.nokitechnologies.MedlabX",
        "productName": "MedlabX",
        "copyright": "Copyright @ 2024 - Noki Technologies Pvt Ltd",
        "asar": true,
        "directories": {
            "buildResources": "src"
        },
        "extraResources": [
            {
                "from": "./.env",
                "to": "resources/.env"
            },
            {
                "from": "./certificates",
                "to": "resources/certificates"
            },
            {
                "from": "./public/config.json",
                "to": "resources/config.json"
            },
            {
                "from": "./public/skeleton/Config Files/CssConfig.json",
                "to": "resources/CssConfig.json"
            },
            {
                "from": "./public/restore-files.ps1",
                "to": "resources/restore-files.ps1"
            }
        ],
        "mac": {
            "icon": "public/logo_X.icns",
            "target": [
                "dmg"
            ]
        },
        "linux": {
            "target": ["AppImage", "deb", "rpm"],
            "icon": "public/logo_X.png"
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64"
                    ]
                }
            ],
            "icon": "public/logo_X.ico"
        },
        "nsis": {
            "oneClick": false,
            "perMachine": true,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true,
            "installerIcon": "public/logo_X.ico",
            "uninstallerIcon": "public/logo_X.ico",
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "shortcutName": "MedlabX"
        },
        "files": [
            "package.json",
            "public/electron.js",
            "build/**/*",
            "public/**/*",
            "server.key",
            "server.pem",
            "rootCA.pem",
            "index.html",
            "node_modules/**/*",
            "!public/config.json",
            "!public/skeleton/Config Files/CssConfig.json",
            "!.env",
            "!public/restore-files.ps1"
        ],
        "publish": {
            "provider": "github",
            "repo": "MedlabX",
            "owner": "OctopodaSolutions",
            "private": true
        },
        "artifactName": "${productName}-Setup-v${version}.${ext}"
    },
    "output": {
        "publicPath": "./"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            "last 1 electron version",
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 electron version",
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@babel/plugin-syntax-import-assertions": "^7.22.5",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-flow": "^7.23.3",
        "@babel/preset-react": "^7.26.3",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.2.1",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.2",
        "electron-builder": "^24.3.0",
        "electron-builder-squirrel-windows": "^20.32.0",
        "electron-devtools-installer": "^3.2.0",
        "electron-react-devtools": "^0.5.3",
        "javascript-obfuscator": "^4.1.0",
        "jest": "^27.5.1",
        "jsdoc": "^4.0.3",
        "react-devtools-electron": "^4.7.0",
        "react-router-dom": "^6.18.0",
        "redux-mock-store": "^1.5.4",
        "sass": "^1.82.0",
        "sass-loader": "^16.0.4",
        "style-loader": "^4.0.0",
        "wait-on": "^7.2.0",
        "webpack": "^5.84.0",
        "webpack-cli": "^5.1.1",
        "webpack-dev-server": "^4.15.2",
        "worker-loader": "^3.0.8"
    }
}
